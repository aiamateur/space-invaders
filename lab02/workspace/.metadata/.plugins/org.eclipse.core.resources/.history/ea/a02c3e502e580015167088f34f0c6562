
#include "debounce.h"


uint8_t enable = 0;
uint8_t bouncer_counter = 0;
uint8_t valid_press = 0;

int toggle_bouncer() {
	enable = !enable;
}

int bounce_enabled(){
	return enable;
}

void inc_bouncer(){
	if(++bouncer_counter == 50){
		valid_press = 1;
	}
}

int is_valid(){
	return valid_press;
}

