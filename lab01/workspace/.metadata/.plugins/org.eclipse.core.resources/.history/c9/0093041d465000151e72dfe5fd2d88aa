/*
 * fibonacci.c: simple test application
 */

#include <stdio.h>
#include <stdint.h>
#include "platform.h"

void print(char *str);

int main()
{
    init_platform();
    char fibby;
    uint_32 f = 0;
    uint_32 s = 0;
    uint_32 c = 0;
    while(c<100){
    	xil_printf("Fibonacci %d\n", c, fibby);
    	if(c==0){
    		f = c;
    		c = 1;
    		s = c;
    	}
    	else{
    		f = c;
    		c = f + s;
    		s = c;
    	}
    }

    cleanup_platform();

    return 0;
}
